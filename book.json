{
  "title": "amtb",
  "description" : "学佛资料",
  "author": "王顶",
  "language": "zh-hans",
  "plugins": [
    "-lunr",
    "-search",
    "-highlight",
    "-sharing",
    "edit-link",
    "forkmegithub",
    "search-plus",
    "sharing-plus",
    "anchor-navigation-ex",
    "github-buttons@2.1.0",
    "splitter",
    "bsz"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "multipleH1": false
    },
    "github-buttons": {
      "repo": "wangding/amtb",
      "types": ["star"],
      "size": "small"
    },
    "forkmegithub": {
      "url": "https://github.com/wangding/amtb",
      "color": "red"
    },
    "edit-link": {
      "base": "https://github.com/wangding/amtb/blob/master/",
      "label": "编辑本页"
    },
    "sharing": {
      "douban": true,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": true,
      "qzone": true,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": true,
      "whatsapp": false,
      "all": [
        "weibo", "douban", "qzone", "qq"
      ]
    },
    "bsz": {
      "copyright": "2018&nbsp;-&nbsp;<span class=\"bsz-this-year\"></span>&nbsp;by&nbsp;<a href=\"https://i.wangding.in\">王顶</a>.&nbsp;All rights reserved."
    }
  }
}
